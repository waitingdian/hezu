.prelease .cu-form-group .title {
  min-width: calc(4em + 15px);
}
.prelease .footer {
  width: 100%;
}
.prelease .row-arow:after {
  font-family: cuIcon;
  display: block;
  content: "\E6A3";
  position: absolute;
  font-size: 17px;
  color: #8799a3;
  line-height: 50px;
  width: 30px;
  text-align: center;
  top: 0;
  bottom: 0;
  right: 5px;
  margin: auto;
}
.prelease .cu-form-group {
  position: relative;
  padding-top: 38rpx;
}
.prelease .cu-form-group .error-tip {
    position: absolute;
    left: 34rpx;
    bottom: -44rpx;
    color: #dd524d;
    z-index: 1;
    font-size: 24rpx;
}

