<view class="rentdetail"><cu-custom vue-id="1" bgColor="uni-custom-header-color" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content','right']}}"><view slot="backText" data-event-opts="{{[['tap',[['BackPage',['$event']]]]]}}" bindtap="__e">返回</view><view style="line-height:100rpx;" slot="content">详情</view><view slot="right"><block wx:if="{{userInfo.user_id!=detailInfo.leader_id}}"><view class="padding-right-lg report-btn" data-target="DialogModal1" data-event-opts="{{[['tap',[['showModal',['$event']]]]]}}" bindtap="__e">举报</view></block></view></cu-custom><block wx:if="{{detailInfo&&detailInfo.id}}"><view class="content padding-bottom-lg"><view class="card-wraper"><swiper class="{{['screen-swiper',dotStyle?'square-dot':'round-dot']}}" indicator-dots="{{true}}" circular="{{true}}" autoplay="{{true}}" interval="3000" duration="500"><block wx:for="{{detailInfo.imgUrlList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item data-event-opts="{{[['tap',[['ViewImage',['$event']]]]]}}" bindtap="__e"><image src="{{item+'?imageView2/1/w/800/h/434'}}" mode="aspectFill"></image></swiper-item></block></swiper></view><view class="cu-bar detail-title"><text class="text-cut">{{detailInfo.title}}</text></view><view class="useringo"><view><text class="label">姓名:</text>{{''+detailInfo.leader}}</view><view><text class="label">性别:</text>{{''+(detailInfo.sex==1?'男':'女')}}</view><view><text class="label">学校:</text>{{''+detailInfo.college}}</view><view><text class="label">毕业时间:</text>{{''+(detailInfo.end_time||'暂无')}}</view><view><text class="label">房间数:</text>{{''+detailInfo.cotenant_count+"间"}}</view><block wx:if="{{detailInfo.status=='0'}}"><view><text class="label">租房状态:</text>正在出租</view></block><block wx:if="{{detailInfo.status=='10'}}"><view><text class="label">租房状态:</text>租房完成</view></block><block wx:if="{{detailInfo.status=='20'}}"><view><text class="label">租房状态:</text>租房解散(取消)</view></block><view><text class="label">地区:</text>{{detailInfo.province+" "+detailInfo.city+" "+detailInfo.district}}</view><view data-event-opts="{{[['tap',[['openLocation',['$event']]]]]}}" bindtap="__e"><text class="label">位置:</text><text class="lg text-gray cuIcon-location">{{detailInfo.address_name}}</text></view><view><block wx:if="{{detailInfo.cotenant_description}}"><text class="label">个人描述:</text></block></view><block wx:if="{{detailInfo.cotenant_description}}"><view class="desc-content">{{detailInfo.cotenant_description}}</view></block><view><block wx:if="{{detailInfo.chamber_description}}"><text class="label">租房描述:</text></block></view><block wx:if="{{detailInfo.chamber_description}}"><view class="desc-content">{{detailInfo.chamber_description}}</view></block></view><block wx:if="{{detailInfo.cotenant_list&&detailInfo.cotenant_list.length}}"><view class="label-title">合租信息</view></block><view class="rent-list margin-top-sm"><block wx:if="{{detailInfo.cotenant_list&&detailInfo.cotenant_list.length}}"><view class="rent-item header" style="color:#606266;margin-bottom:0;"><text class="name">昵称</text><text class="sex">性别</text><text class="college">大学</text><text class="hobby">兴趣爱好</text></view></block><block wx:for="{{detailInfo.cotenant_list}}" wx:for-item="rent" wx:for-index="__i0__" wx:key="id"><view class="rent-item"><text class="name">{{rent.nick_name}}</text><text class="sex">{{rent.sex==1?'男':'女'}}</text><text class="college">{{rent.college}}</text><text class="hobby">{{rent.interest}}</text></view></block></view><block wx:if="{{userInfo.user_id!=detailInfo.leader_id}}"><view class="padding-top-sm" style="text-align:right;"><button class="cu-btn bg-blue margin-left" data-target="DialogModal2" data-event-opts="{{[['tap',[['showModal',['$event']]]]]}}" bindtap="__e">申请加入</button><button data-event-opts="{{[['tap',[['goChat',['$event']]]]]}}" class="cu-btn margin-left" bindtap="__e">聊一聊</button></view></block><block wx:else><view class="padding-top-sm" style="text-align:right;"><button data-event-opts="{{[['tap',[['doDismiss',['$event']]]]]}}" class="cu-btn line-blue margin-left" bindtap="__e">解散该团</button></view></block></view></block><block wx:if="{{modalName=='DialogModal1'}}"><view><view class="{{['cu-modal',modalName=='DialogModal1'?'show':'']}}"><view class="cu-dialog"><view class="cu-bar bg-white justify-end"><view class="content padding-top-sm">举报</view><view data-event-opts="{{[['tap',[['hideModal',['$event']]]]]}}" class="action" bindtap="__e"><text class="cuIcon-close text-red"></text></view></view><view class="cu-form-group margin-top padding-xl margin-bottom" style="text-align:left;"><textarea maxlength="-1" placeholder="请输入举报内容" data-event-opts="{{[['input',[['textareaAInput',['$event']]]]]}}" bindinput="__e"></textarea></view><view class="cu-bar bg-white justify-end"><view class="action"><button data-event-opts="{{[['tap',[['hideModal',['$event']]]]]}}" class="cu-btn line-blue text-green" bindtap="__e">取消</button><button class="cu-btn bg-blue margin-left" disabled="{{!reportContent}}" data-event-opts="{{[['tap',[['reported',['$event']]]]]}}" bindtap="__e">确定</button></view></view></view></view></view></block><block wx:if="{{modalName=='DialogModal2'}}"><view class="{{['cu-modal',modalName=='DialogModal2'?'show':'']}}"><view class="cu-dialog"><view class="cu-bar bg-white justify-end"><view class="content padding-top-sm">申请加入</view><view data-event-opts="{{[['tap',[['hideModal',['$event']]]]]}}" class="action" bindtap="__e"><text class="cuIcon-close text-red"></text></view></view><view class="padding-xl">确定申请加入该组团?</view><view class="cu-bar bg-white justify-end"><view class="action"><button data-event-opts="{{[['tap',[['hideModal',['$event']]]]]}}" class="cu-btn line-blue text-green" bindtap="__e">取消</button><button data-event-opts="{{[['tap',[['join',['$event']]]]]}}" class="cu-btn bg-blue margin-left" bindtap="__e">确定</button></view></view></view></view></block></view>