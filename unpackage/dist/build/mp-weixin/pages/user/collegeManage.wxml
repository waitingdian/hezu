<view class="content"><cu-custom vue-id="1" bgColor="uni-custom-header-color" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText" data-event-opts="{{[['tap',[['BackPage',['$event']]]]]}}" bindtap="__e">返回</view><view style="line-height:100rpx;" slot="content">{{title}}</view></cu-custom><view class="row b-b"><text class="tit">大学名称</text><picker value="{{colegeIndex}}" range="{{collegeList}}" range-key="college_name" data-event-opts="{{[['change',[['colegeChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+(colegeIndex>-1?collegeList[colegeIndex].college_name:'请选择')+''}}</view></picker></view><view class="row b-b"><text class="tit">入学时间</text><picker value="{{startIndex}}" range="{{startDateList}}" data-event-opts="{{[['change',[['startDateChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+(startIndex>-1?startDateList[startIndex]:'请选择')+''}}</view></picker></view><view class="row b-b"><text class="tit">毕业时间</text><picker value="{{endIndex}}" range="{{endDateList}}" data-event-opts="{{[['change',[['endDateChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+(endIndex>-1?endDateList[endIndex]:'请选择')+''}}</view></picker></view><view class="row default-row"><text class="tit">设为默认</text><switch checked="{{collegeData.is_default}}" color="#fa436a" data-event-opts="{{[['change',[['switchChange',['$event']]]]]}}" bindchange="__e"></switch></view><button data-event-opts="{{[['tap',[['confirm',['$event']]]]]}}" class="add-btn" bindtap="__e">提交</button></view>