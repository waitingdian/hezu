<view class="user-page"><cu-custom vue-id="1" bgColor="uni-custom-header-color" isBack="{{true}}" bind:__l="__l" vue-slots="{{['content']}}"><view slot="content">我的信息</view></cu-custom><view class="user-info"><image class="avator" src="{{userInfo.user_head?userInfo.user_head+'?imageView2/1/w/200/h/200':defaultAvator+'?imageView2/1/w/200/h/200'}}"></image><view class="name-wraper"><text>{{userInfo.user_name}}</text></view></view><view class="cu-list menu" style="margin-top:30rpx;"><view data-event-opts="{{[['tap',[['goUserSetting',['$event']]]]]}}" class="cu-item" bindtap="__e"><view class="content content-arrow"><text class="text-grey">个人信息</text></view></view><view data-event-opts="{{[['tap',[['clear',['$event']]]]]}}" class="cu-item" bindtap="__e"><view class="content content-arrow"><text class="text-grey">清除缓存</text></view></view><view class="cu-item" data-target="DialogModal1" data-event-opts="{{[['tap',[['showModal',['$event']]]]]}}" bindtap="__e"><view class="content content-arrow"><text class="text-grey">意见反馈</text></view></view><block wx:if="{{modalName=='DialogModal1'}}"><view><view class="{{['cu-modal',modalName=='DialogModal1'?'show':'']}}"><view class="cu-dialog"><view class="cu-bar bg-white justify-end"><view class="content padding-top-sm">用户建议反馈</view><view data-event-opts="{{[['tap',[['hideModal',['$event']]]]]}}" class="action" bindtap="__e"><text class="cuIcon-close text-red"></text></view></view><view class="cu-form-group margin-top padding-xl margin-bottom" style="text-align:left;"><textarea maxlength="-1" placeholder="请输入您想反馈的内容" data-event-opts="{{[['input',[['textareaAInput',['$event']]]]]}}" bindinput="__e"></textarea></view><view class="cu-bar bg-white justify-end"><view class="action"><button data-event-opts="{{[['tap',[['hideModal',['$event']]]]]}}" class="cu-btn line-blue text-green" bindtap="__e">取消</button><button class="cu-btn bg-blue margin-left" disabled="{{!content}}" data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" bindtap="__e">确定</button></view></view></view></view></view></block><view data-event-opts="{{[['tap',[['about',['$event']]]]]}}" class="cu-item" bindtap="__e"><view class="content content-arrow"><text class="text-grey">关于校友合租</text></view></view></view></view>