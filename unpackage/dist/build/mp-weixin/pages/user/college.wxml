<view class="content b-t"><cu-custom vue-id="1" bgColor="uni-custom-header-color" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText" data-event-opts="{{[['tap',[['BackPage',['$event']]]]]}}" bindtap="__e">返回</view><view style="line-height:100rpx;" slot="content">我的学校</view></cu-custom><view class="example-body"><block wx:for="{{collegeList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="example-box"><uni-card vue-id="{{'2-'+index}}" bind:__l="__l" vue-slots="{{['default']}}"><view class="wrapper"><view class="address-box"><block wx:if="{{item.is_default}}"><text class="tag">默认</text></block><text class="address">{{item.college_name}}</text></view><view class="u-box"><text>{{"入学时间:"+item.start_time}}</text><view class="_br"></view><text>{{"毕业时间:"+item.end_time}}</text></view></view><view align="center" color="#D3D3D3" SIZE="1" class="_hr"></view><view style="float:right;"><text data-event-opts="{{[['tap',[['addCollege',['edit','$0'],[[['collegeList','',index]]]]]]]}}" class="lg text-gray cuIcon-edit" style="font-size:36rpx;padding-left:38rpx;cursor:pointer;" bindtap="__e"><text class="padding-left-xs" style="font-size:24rpx;">修改</text></text><text data-event-opts="{{[['tap',[['deleteCollege',['$0'],[[['collegeList','',index,'id']]]]]]]}}" class="lg text-gray cuIcon-delete" style="font-size:36rpx;padding-left:38rpx;cursor:pointer;" bindtap="__e"><text class="padding-left-xs" style="font-size:24rpx;">删除</text></text></view></uni-card></view></block></view><button data-event-opts="{{[['tap',[['addCollege',['add']]]]]}}" class="add-btn" bindtap="__e">新增学校</button></view>