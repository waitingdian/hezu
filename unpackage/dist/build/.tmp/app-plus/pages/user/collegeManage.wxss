  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  -webkit-transition: all 0.3s ease;
  transition: all 0.3s ease;
  -webkit-transform: translateY(100%);
          transform: translateY(100%);
  z-index: 3000;
}
.mpvue-picker-view-show {
  -webkit-transform: translateY(0);
          transform: translateY(0);
}
.mpvue-picker__hd {
  display: -webkit-box;
